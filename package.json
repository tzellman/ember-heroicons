{
  "name": "ember-heroicons",
  "version": "0.4.0",
  "description": "Heroicons for Ember.js",
  "keywords": ["ember-addon", "heroicons", "icons", "tailwind"],
  "repository": "https://github.com/tzellman/ember-heroicons.git",
  "license": "MIT",
  "author": "tzellman",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "clean": "rimraf dist coverage package*.ember-try .eslintcache",
    "format": "prettier --write .",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "purge": "npm run clean && rimraf node_modules",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@babel/core": "^7.23.7",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^4.2.0",
    "dom-serializer": "^2.0.0",
    "ember-auto-import": "^2.7.2",
    "ember-cli-babel": "^8.2.0",
    "ember-cli-htmlbars": "^6.3.0",
    "ember-get-config": "2.1.1",
    "ember-modifier": "4.1.0",
    "glob": "10.3.4",
    "heroicons": "^2.1.1",
    "htmlparser2": "9.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-proposal-decorators": "^7.23.7",
    "@commitlint/cli": "17.7.1",
    "@commitlint/config-conventional": "17.7.0",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^3.2.1",
    "@embroider/test-setup": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "autoprefixer": "10.4.15",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.2",
    "ember-cli": "~5.6.0",
    "ember-cli-clean-css": "3.0.0",
    "ember-cli-dependency-checker": "^3.3.2",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-postcss": "^8.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^8.2.0",
    "ember-qunit": "8.0.2",
    "ember-resolver": "11.0.1",
    "ember-source": "~5.6.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.13.0",
    "ember-truth-helpers": "4.0.3",
    "ember-try": "^3.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-ember": "^11.12.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-qunit": "^8.0.1",
    "husky": "4.3.8",
    "lint-staged": "^14.0.1",
    "loader.js": "^4.7.0",
    "prettier": "^3.2.4",
    "qunit": "^2.20.0",
    "qunit-dom": "^2.0.0",
    "rimraf": "5.0.1",
    "stylelint": "^15.11.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-prettier": "^4.1.0",
    "tailwindcss": "3.3.3",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "ember-source": "^4.8.3 || >= 5.0.0"
  },
  "engines": {
    "node": ">= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://tzellman.github.io/ember-heroicons/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "lint-staged": {
    "*.{js,json,md,css,yml}": "prettier --write"
  }
}
