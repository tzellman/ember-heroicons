{
  "name": "ember-heroicons",
  "version": "0.3.0",
  "description": "Heroicons for Ember.js",
  "keywords": ["ember-addon", "heroicons", "icons", "tailwind"],
  "repository": "https://github.com/tzellman/ember-heroicons.git",
  "license": "MIT",
  "author": "tzellman",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "clean": "rimraf dist coverage package*.ember-try .eslintcache",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "purge": "npm run clean && rimraf node_modules",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each"
  },
  "dependencies": {
    "@ember/render-modifiers": "^2.0.5",
    "broccoli-file-creator": "^2.1.1",
    "broccoli-merge-trees": "^4.2.0",
    "dom-serializer": "^2.0.0",
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-get-config": "2.1.1",
    "glob": "8.0.3",
    "heroicons": "^2.0.16",
    "htmlparser2": "^8.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "autoprefixer": "^10.4.14",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.6.1",
    "ember-cli": "~4.11.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-postcss": "^8.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.2.0",
    "ember-resolver": "^10.0.0",
    "ember-source": "^4.11.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.6.0",
    "ember-truth-helpers": "^3.1.1",
    "ember-try": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-ember": "^11.4.7",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "husky": "4.3.8",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.4",
    "pretty-quick": "^3.1.3",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "rimraf": "^4.4.0",
    "tailwindcss": "^3.2.7",
    "webpack": "^5.76.1"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://tzellman.github.io/ember-heroicons/"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npx commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "pretty-quick --staged --config .prettierrc"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  }
}
